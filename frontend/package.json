  "license": "SEE LICENSE IN <https://www.creative-tim.com/product/notus-pro-react>",
  "homepage": "https://demos.creative-tim.com/notus-pro-react/",
  "main": "./index.js",
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.3",
    "@popperjs/core": "2.9.2",
    "@tailwindcss/forms": "0.3.2",
    "autoprefixer": "10.2.5",
    "chart.js": "3.1.0",
    "classnames": "2.3.1",
    "postcss": "8.2.10",
    "prop-types": "15.7.2",
